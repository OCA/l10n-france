<data>

<!-- Add codes to form view -->
<record id="fr_intrastat_product_intrastat_type_form" model="ir.ui.view">
    <field name="name">fr.intrastat.product.intrastat.type.form</field>
    <field name="model">report.intrastat.type</field>
    <field name="inherit_id" ref="l10n_fr_intrastat_base.fr_intrastat_base_report_intrastat_type_form" />
    <field name="type">form</field>
    <field name="arch" type="xml">
        <field name="active" position="after">
            <field name="procedure_code" />
            <field name="transaction_code" />
        </field>
    </field>
</record>

<!-- Add codes to tree view -->
<record id="fr_intrastat_product_intrastat_type_tree" model="ir.ui.view">
    <field name="name">fr.intrastat.product.intrastat.type.tree</field>
    <field name="model">report.intrastat.type</field>
    <field name="inherit_id" ref="l10n_fr_intrastat_base.fr_intrastat_base_report_intrastat_type_tree" />
    <field name="type">tree</field>
    <field name="arch" type="xml">
        <field name="name" position="after">
            <field name="procedure_code" />
            <field name="transaction_code" />
        </field>
    </field>
</record>

</data>
