<odoo>

    <record id="view_account_move_line_search_subrogation" model="ir.ui.view">
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_account_move_line_filter" />
        <field name="arch" type="xml">
            <field name="journal_id" position="after">
                <field name="subrogation_id" />
            </field>
        </field>
    </record>

    <record id="action_account_move_subrogation" model="ir.actions.act_window">
        <field name="name">Account moves of a subrogation</field>
        <field name="res_model">account.move.line</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
