        </record>
        <record id="demo_contract_mit" model="hr.contract">
            <field name="name">John Smith contrat</field>
            <field name="employee_id" ref="hr.employee_mit"/>
            <field name="type_id" ref="l10n_fr_rup.hr_contract_type_cdi"/>
