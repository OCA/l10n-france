        </record>

        <!--Complementary employee information-->
        <record id="hr.employee_fp" model="hr.employee">
            <field name="birthday" eval="(DateTime.today() - relativedelta(years=30)).strftime('%Y-%m-%d')"/>
            <field name="gender">male</field>
            <field name="country_id" ref="base.fr"/>
            <field name="identification_id">121128687763</field>
            <field name="address_home_id" ref="base.res_partner_2"/>
